NIC_CONFIG_ENABLE=false
NFD_ENABLE=false
OFED_ENABLE=true
CREATE_CONFIG_ONLY=1
USECASE="sriovnet_rdma"
MTU_DEFAULT="9000"
DEVICE_TYPES=( "connectx-7" )
# for maximum E/W RDMA performance each NIC port gets defined as a network
# example device PCI id for connectx-7 = 1021 # interpreted by config as HEX
NETOP_NETLIST=( a,,,0000:08:00.0 b,,,0000:09:00.0 )
